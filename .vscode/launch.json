{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(lldb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/clabdev",
      "args": [
				"debug",
				"${input:runArg}"
			],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb"
    }
  ],
  "inputs": [
    {
      "id": "runArg",
      "type": "promptString",
      "description": "Enter the debug function name for clabdev",
      "default": "function_demo"
    }
  ]
}
